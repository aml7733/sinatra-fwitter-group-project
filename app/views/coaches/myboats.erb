<h1>Here are your boats, <%= current_coach.name %></h1>

<% current_coach.boats.each do |boat| %>
  <h3><%= boat.name %></h3>
  <p>
    <%= boat.rowers.each do |rower|%>
      <%= rower.name %><br>
    <% end %>
  </p><br>
<%= end %>



<br>
<a href="/coaches/logout">Log Out</a>
<br>
<a href="/boat/new"></a>
<br>
<form action="/boat/edit" method="post">
  <label>Enter the name of the boat you'd like to edit:</label>
  <input type="text" name="boat[name]">
  <input type="submit" name="submit" value="submit">
</form>
